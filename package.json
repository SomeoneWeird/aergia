    "fs-extra": "^1.0.0",
    "material-ui": "^0.19.0",
    "node-stream-zip": "^1.3.6",
    "normalize.css": "^7.0.0",
    "open": "0.0.5",
    "otherapp": "^1.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.79.0",
    "request-progress": "^3.0.0",
    "semver": "^5.3.0",
